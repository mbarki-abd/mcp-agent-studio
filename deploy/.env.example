# MCP Agent Studio - Production Environment
# Copy this file to .env and fill in the values

# Domain configuration
DOMAIN=mcp-studio.ilinqsoft.com

# PostgreSQL
POSTGRES_USER=mcp
POSTGRES_PASSWORD=CHANGE_ME_STRONG_PASSWORD
POSTGRES_DB=mcp_agent_studio

# Redis
REDIS_PASSWORD=CHANGE_ME_STRONG_PASSWORD

# JWT Secret (generate with: openssl rand -base64 32)
JWT_SECRET=CHANGE_ME_GENERATE_WITH_OPENSSL

# Encryption key for sensitive data (generate with: openssl rand -hex 32)
ENCRYPTION_KEY=CHANGE_ME_GENERATE_WITH_OPENSSL

# Traefik dashboard auth (generate with: htpasswd -nb admin password)
TRAEFIK_AUTH=admin:$$apr1$$CHANGE_ME

# GitHub Container Registry
GITHUB_REPOSITORY=mbark/mcp-agent-studio
VERSION=latest
